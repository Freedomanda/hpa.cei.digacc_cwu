<core:FragmentDefinition xmlns="sap.m"  xmlns:core="sap.ui.core">
	<Dialog title="{i18n>dataLossDialogTitle}" state="Warning" type="Message" >
		<content>
			<Text text="{i18n>saveFirstMessage}"/>
		</content>
		<buttons>
			<Button id="dataLossYesButton" text="{i18n>yesButton}" tooltip="{i18n>yesButton}" press="onSaveNav" />
			<Button id="dataLossNoButton" text="{i18n>noButton}" tooltip="{i18n>noButton}" press="onNoSaveNav"/>
			<Button id="dataLossCancelButton" text="{i18n>cancelButton}" tooltip="{i18n>cancelButton}" press="onCancelNav"/>
		</buttons>
	</Dialog>
</core:FragmentDefinition>